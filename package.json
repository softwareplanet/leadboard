{
  "name": "leadboard-server",
  "main": "server.js",
  "license": "MIT",
  "scripts": {
    "test": "jest --runInBand",
    "client-install": "npm install --prefix client",
    "coverage": "npm test -- --coverage && opn coverage/lcov-report/index.html",
    "server": "nodemon $NODE_DEBUG_OPTION server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "start": "node server.js",
    "heroku-postbuild": "cd client && npm install && npm run build"
  },
  "jest": {
    "testEnvironment": "node",
    "setupTestFrameworkScriptFile": "./test/setup.js",
    "testPathIgnorePatterns": [
      "/client/",
      "/node_modules/"
    ]
  },
  "dependencies": {
    "babel-core": "^6.21.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-1": "^6.16.0",
    "babel-runtime": "^6.20.0",
    "bcrypt": "^3.0.0",
    "body-parser": "^1.15.2",
    "compression": "^1.6.2",
    "connect-mongo": "^2.0.1",
    "cors": "^2.8.1",
    "cross-env": "^3.1.4",
    "express": "^4.13.4",
    "express-session": "^1.15.6",
    "http": "0.0.0",
    "jsonwebtoken": "^8.2.0",
    "lodash.isempty": "^4.4.0",
    "mailgun-js": "^0.20.0",
    "material": "^0.3.0",
    "mdbootstrap": "^4.5.8",
    "mdbreact": "^4.7.0",
    "moment": "^2.22.2",
    "mongoose": "^4.4.5",
    "mongoose-error-parser": "^1.0.0",
    "morgan": "^1.7.0",
    "node-schedule": "^1.3.0",
    "nunjucks": "^3.1.3",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "q": "^1.5.1",
    "save": "^2.3.2",
    "validator": "^10.5.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "babel-jest": "18.0.0",
    "concurrently": "^3.6.1",
    "eslint": "^5.4.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "jest-cli": "18.1.0",
    "nodemon": "^1.11.0",
    "redux-mock-store": "^1.5.3",
    "sinon": "^6.1.3",
    "supertest": "^2.0.1"
  }
}
